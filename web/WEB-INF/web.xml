<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>HttpSessionListener</description>
        <listener-class>techquiz.controller.userCountListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>RegistrationControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.RegistrationControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.LoginControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.AdminControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.StudentControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.TeacherControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetQuestionsControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.SetQuestionsControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TakeTestControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.TakeTestControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditQuestionsControllerServlet</servlet-name>
        <servlet-class>techquiz.controller.EditQuestionsControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowResultController</servlet-name>
        <servlet-class>techquiz.controller.ShowResultController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>techquiz.controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorldChatController</servlet-name>
        <servlet-class>techquiz.controller.WorldChatController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SettingsController</servlet-name>
        <servlet-class>techquiz.controller.SettingsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationControllerServlet</servlet-name>
        <url-pattern>/RegistrationControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControllerServlet</servlet-name>
        <url-pattern>/LoginControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminControllerServlet</servlet-name>
        <url-pattern>/AdminControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentControllerServlet</servlet-name>
        <url-pattern>/StudentControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherControllerServlet</servlet-name>
        <url-pattern>/TeacherControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetQuestionsControllerServlet</servlet-name>
        <url-pattern>/SetQuestionsControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TakeTestControllerServlet</servlet-name>
        <url-pattern>/TakeTestControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditQuestionsControllerServlet</servlet-name>
        <url-pattern>/EditQuestionsControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowResultController</servlet-name>
        <url-pattern>/ShowResultController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorldChatController</servlet-name>
        <url-pattern>/WorldChatController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SettingsController</servlet-name>
        <url-pattern>/SettingsController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
